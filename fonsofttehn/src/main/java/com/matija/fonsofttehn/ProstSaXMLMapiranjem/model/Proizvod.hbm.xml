<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.matija.fonsofttehn.ProstSaXMLMapiranjem.model">

	<class name="Proizvod" table="Proizvod" >
            
		<id name="proizvodID" column="proizvodID">
             <generator class="assigned"/>
        </id>
        <property name="naziv" column="naziv"/>
        <property name="cena" column="cena" type="double"/>
	</class>
	
</hibernate-mapping>